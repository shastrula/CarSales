                </library>
                <library dirs="200">
                    <file>${file.reference.commons-beanutils.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.log4j-1.2.13.jar}</file>
                    <path-in-war>WEB-INF/lib</path-in-war>
                </library>
            </web-module-libraries>
            <web-module-additional-libraries>
                <library dirs="200">
                    <file>${file.reference.json-lib-2.1-jdk13.jar}</file>
                    <path-in-war>/</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.commons-lang.jar}</file>
                    <path-in-war>/</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.ezmorph-1.0.3.jar}</file>
                    <path-in-war>/</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.commons-collections.jar}</file>
                    <path-in-war>/</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.commons-beanutils.jar}</file>
                    <path-in-war>/</path-in-war>
                </library>
                <library dirs="200">
                    <file>${file.reference.log4j-1.2.13.jar}</file>
                    <path-in-war>/</path-in-war>
                </library>
            </web-module-additional-libraries>
            <source-roots>
                <root id="src.dir"/>
            </source-roots>
            <test-roots>
                <root id="test.src.dir"/>
            </test-roots>
        </data>
        <spring-data xmlns="http://www.netbeans.org/ns/spring-data/1">
            <config-files>
                <config-file>web/WEB-INF/applicationContext.xml</config-file>
                <config-file>web/WEB-INF/dispatcher-servlet.xml</config-file>
                <config-file>web/WEB-INF/inventory.xml</config-file>
            </config-files>
            <config-file-groups>
                <config-file-group name="Default Group">
                    <config-file>web/WEB-INF/applicationContext.xml</config-file>
                    <config-file>web/WEB-INF/dispatcher-servlet.xml</config-file>
                </config-file-group>
            </config-file-groups>
        </spring-data>
    </configuration>
</project>
